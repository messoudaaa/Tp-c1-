#include <stdio.h>
 int main()
  { 
   int mat [10][10], matrix[10][10];
    int i ,j ;
     for(i=0;i<10;i++)
      {
       for(j=0;j<10;j++)
       scanf("%d",& mat[i][j]);
       printf("%d",& mat[i][j]);
      }
      printf("\n");
      for(i=0;i<10;i++)
      {
       for(j=0;j<10;j++)
       matrix [j][i]=mat[i][j];
      }
      for (i=0;i<10;i++)
      {
       for(j=0;j<10;j++)
       printf("%d" ,& matrix[i][j]);
      }
       printf("\n");
    return 0;
   }
