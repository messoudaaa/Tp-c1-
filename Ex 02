#include <stdio.h >
int main () {
int matrix[10] [10]
int i ,j ,t;
for(i=0;i<10;i++)
     for(j=0;j<10;j++)
           if(i=j)
       matrix[i][j]=-20;
       else  
       if (i>j)
       t=matrix[i][j]
       matrix [i][j]=matrix [j][i]
       for(i=0;i<10;i++)
             for (j=0;j<10;j++)
             printf("%c ,/t",matrix[i][j]);
             printf("\n");
             return 0;
               }
